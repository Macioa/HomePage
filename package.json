{
  "name": "homepage",
  "version": "1.0.0",
  "description": "HomePage Rewrite",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/jest",
    "build": "npx webpack --silent || true && webpack -p",
    "build-full": "rm -rf dist && find src -name *.d.ts -type f -delete && npm run-script build && npm test -- --u",
    "dev": "npm run build-full && npm start",
    "quick": "webpack && npm start",
    "start": "node index || npm install && npm run-script build && node index"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Macioa/HomePage.git"
  },
  "author": "Ryan Montgomery",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/Macioa/HomePage/issues"
  },
  "homepage": "https://github.com/Macioa/HomePage#readme",
  "jest": {
    "setupFiles": [
      "jest-webgl-canvas-mock"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      ".css$": "<rootDir>/src/styles.css.d.ts",
      ".svg$": "<rootDir>/ops/svgHandler.js"
    },
    "transform": {
      "\\.(ts|tsx|css)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$",
    "verbose": true
  },
  "dependencies": {
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/cannon": "^0.1.4",
    "@types/jest": "^24.0.17",
    "@types/node": "^12.6.9",
    "@types/react": "^16.8.24",
    "@types/react-dom": "^16.8.5",
    "@types/react-test-renderer": "^16.8.3",
    "cannon": "^0.6.2",
    "css-loader": "^1.0.1",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "jest-css-modules": "^2.1.0",
    "jest-webgl-canvas-mock": "^0.2.2",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-test-renderer": "^16.8.6",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "three": "^0.107.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6"
  }
}
