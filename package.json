    "url": "https://github.com/Macioa/HomePage/issues"
  },
  "homepage": "https://github.com/Macioa/HomePage#readme",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "css-loader": "^1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "@types/node": "^12.6.9",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.0.4",
    "typings-for-css-modules-loader": "^1.7.0",
    "typescript": "^3.5.3",
